<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/colorButtonSolidPress" />
            <stroke android:width="1dp" android:color="#ead605" />
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="1dp" android:color="#e6b12b" />
            <solid android:color="#f5df00" />
            <corners android:radius="5dp"/>
        </shape>
    </item>

</selector>